<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id ="brokerFactory" class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfoFactory">
		<property name="initiatorBrokerMapping">
			<map>
				<entry key ="AutoPilotToLiqFi">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.BROKER.URL}"/>
						<property name="user" value="${TOPIC.BROKER.USER}"/>
						<property name="password" value="${TOPIC.BROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="A-G" value="EU.LIQUIFI.INST1.ORDER"/>
								<entry key="H-K" value="EU.LIQUIFI.INST2.ORDER"/>
								<entry key="L-U" value="EU.LIQUIFI.INST3.ORDER"/>
								<entry key="V-Z" value="EU.LIQUIFI.INST4.ORDER"/>
							</map>
						</property>
 					</bean>
 				</entry>
				<entry key ="AutoPilotToLiqFiQFFLite">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.QFFLITEBROKER.URL}"/>
						<property name="user" value="${TOPIC.QFFLITEBROKER.USER}"/>
						<property name="password" value="${TOPIC.QFFLITEBROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="A-G" value="EU.LIQUIFI.INST1.ORDER"/>
							</map>
						</property>
 					</bean>
 				</entry>
				<entry key ="AutoPilotToLiqFiAdmin">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.BROKERADMIN.URL}"/>
						<property name="user" value="${TOPIC.BROKERADMIN.USER}"/>
						<property name="password" value="${TOPIC.BROKERADMIN.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="A-G" value="EU.LIQUIFI.ADMINGUITOCORE.P1"/>
								<entry key="H-M" value="EU.LIQUIFI.ADMINGUITOCORE.P2"/>
								<entry key="N-U" value="EU.LIQUIFI.ADMINGUITOCORE.P3"/>
								<entry key="V-Z" value="EU.LIQUIFI.ADMINGUITOCORE.P4"/>
							</map>
						</property>
						<property name="replyAcceptor" value="LiqFiToAutoPilot" />
 					</bean>
 				</entry>
 			</map>	
		</property>

		<!-- used if 49 is set, but 50 isn't -->
		<property name="senderSubIDMapping">
			<map>
				<entry key ="CS">

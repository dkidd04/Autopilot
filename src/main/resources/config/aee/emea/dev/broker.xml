					</map>
				</entry>
				<entry key ="ATD">
					<map>
						<entry key="A-Z" value="ATD_1"/>
					</map>
				</entry>
				<entry key ="EDTS">
					<map>
						<entry key="A-Z" value="LQFI_EDTS_C1S1"/>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id ="brokerFactory" class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfoFactory">
		<property name="initiatorBrokerMapping">
			<map>
				<entry key ="AutoPilotToLiqFi">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.BROKER.URL}"/>
						<property name="user" value="${TOPIC.BROKER.USER}"/>
						<property name="password" value="${TOPIC.BROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="0-9" value="${TOPIC.AEE.INBOUND}"/>
							</map>
						</property>
 					</bean>
 				</entry>
				<entry key ="AutoPilotToLiqFiAdmin">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.BROKER.URL}"/>
						<property name="user" value="${TOPIC.BROKER.USER}"/>
						<property name="password" value="${TOPIC.BROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="0-9" value="${TOPIC.AEE.ADMIN}"/>
							</map>
						</property>
 					</bean>
 				</entry>
 			</map>	
		</property>

		<property name="acceptorBrokerMapping">
			<map>
				<entry key ="LiqFiToAutoPilot">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${TOPIC.BROKER.URL}"/>
						<property name="user" value="${TOPIC.BROKER.USER}"/>
						<property name="password" value="${TOPIC.BROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="0-9" value="${AUTOPILOT.INBOUND.TOPIC}"/>
							</map>
						</property>
 					</bean>
				</entry>
				<entry key ="LiqFiToEXCH">
					<bean class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotBrokerInfo">
						<property name="brokerURL" value="${XSVC.BROKER.URL}"/>
						<property name="user" value="${XSVC.BROKER.USER}"/>
						<property name="password" value="${XSVC.BROKER.PASSWORD}"/>
						<property name="setup">
							<map>
								<entry key="0-9" value="${XSVC.INBOUND.TOPIC}"/>
							</map>
						</property>
 					</bean>
				</entry>
 			</map>
		</property>
		<property name='senderCompID' value='AUTOPILOT'></property>
		<property name='senderSubID' value='AUTOPILOT1'></property>		
	</bean>
	
	<bean id ="socketFactory" class = "com.citigroup.liquifi.autopilot.messaging.AutoPilotSocketConnectionFactory">
		<property name="socketList">
			<list> 
				<bean class = "com.citigroup.liquifi.autopilot.messaging.SocketConnectionInfo">
					<property name="host" value="osnuleqlf2.jpn.nsroot.net"/>
					<property name="port" value="18003"/>
					<property name="socketID" value="TOBtoAutoPilot"/>
				</bean>
			</list>
		</property>

		<property name="sockets">
			<list> 
				<bean class = "com.citigroup.liquifi.autopilot.socket.PMMSocket">
					<property name="host" value="localhost"/>
					<property name="port" value="18103"/>
					<property name="outbound" value="LiqFiToATDSocket"/>
					<property name="inbound" value="AutoPilotToLiqFiATDSocket"/>
				</bean>
			</list>
		</property>
	</bean>
</beans>
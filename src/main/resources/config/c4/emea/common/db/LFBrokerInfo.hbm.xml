<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.citigroup.liquifi.autopilot.db.dao">
  <class name="com.citigroup.liquifi.autopilot.db.dao.BrokerInfo" table = "LFBrokerInfo">
  	<id name ="brokerName" type = "java.lang.String" column ="BrokerName">
  	</id>
  	<property name="brokerURL" type="java.lang.String" column = "BrokerURL" length="50"/>
  	<property name="userID" type="java.lang.String" column = "EncryptedUserID" length="255"/>
  	<property name="password" type="java.lang.String" column="EncryptedPass" length="255"/>
  	<property name="active" type="java.lang.Boolean" column="Active"/>
  	<property name="comments" type="java.lang.String" column="Comments" length="255"/>
  	
  
  </class>
</hibernate-mapping>
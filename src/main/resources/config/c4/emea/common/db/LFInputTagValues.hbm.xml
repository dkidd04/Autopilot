<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.citigroup.liquifi.entities">

    <class name="com.citigroup.liquifi.entities.LFTag" table="LFInputTagValues">
    	<composite-id>
    	    <key-property name="testID" column = "TestID"/>
        	<key-property name="actionSequence" column = "ActionSequence"/>
        	<key-property name="tagID" column = "TagID"/>
    	</composite-id>
        <property name="tagValue" column = "InputTagValue"/>

    </class>

</hibernate-mapping>